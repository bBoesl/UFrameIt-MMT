theory DistanceFact =
    include ☞http://BenniDoes.Stuff/?UnorderdPair ❙
    distanceFact = [a: V,b: V, c : ℝ] ⊦ ( ( d a b) ≐ c )❘ # DF 1 2 3 ❙
