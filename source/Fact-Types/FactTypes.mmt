theory UnorderdPair =
    VPair : type  ❙
    VPair_create : V ⟶ V ⟶ VPair ❘ # ⋖ 1 2 ⋗ ❙
    VPair_unorderd: {a : V,b : V}  ⊦ (  true ≐ ( ⋖ a b ⋗ ≐ ⋖ b a ⋗ )  )  ❘ role ForwardRule ❙
