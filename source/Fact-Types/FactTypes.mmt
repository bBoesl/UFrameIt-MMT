theory AngleFact =
    include ☞http://BenniDoes.Stuff/?UnorderdPair ❙
    angleFact = [a: V, b: V, c: V, v : ℝ] ⊦ ( angleBetween a b c ) ≐ v ❘ # AF 1 2 3 4❙
